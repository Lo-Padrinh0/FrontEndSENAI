algoritmo "aula7ATIVIDADE2"

var
   numero : vetor [1..10] de caractere
   contador : inteiro

inicio

para contador <- 1 ate 10 faca
    escreval("Insira o seu ",contador," ° número")
    leia(numero[contador])
  limpaTela()
fimPara

para contador <- 10 ate 1 passo -1 faca
    escreval(numeros[contador])
fimPara


fimalgoritmo
Algoritmo "revisãoATIVIDADE2"

Var
   usuario, senha, figado, couve : caractere

Inicio
      escreval("Digite o usuario :")
         leia(usuario)
      escreval("Digite a senha :")
         leia(senha)
      
      se (usuario = "figado") e (senha = "couve")entao
      escreval("ACESSO VÁLIDO")
      senao
         escreval("ACESSO INVÁLIDO")
    fimSe
Fimalgoritmo

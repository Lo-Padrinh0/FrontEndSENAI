Algoritmo "ATIVIDADE3"

Var
       contador : inteiro
    
Inicio
//contar ate 100
   contador <- 1
   
   enquanto (contador <= 100) faca
   
   se (contador % 10 = 0) entao
       escreval(contador, " É divisivel por 10")
   senao
       escreval(contador)
   fimSe
   
   contador <- contador + 1

   fimEnquanto
   
//cada multiplo de 10, imprime mensagem

Fimalgoritmo
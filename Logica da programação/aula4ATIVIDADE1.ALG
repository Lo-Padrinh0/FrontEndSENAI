Algoritmo "ATIVIDADE1"

Var
      somaTotal, numeroAtual : inteiro

Inicio
   enquanto (numeroAtual <= 10) faca
      escreval(numeroAtual)
      somaTotal <- somaTotal + numeroAtual
      numeroAtual <- numeroAtual + 1
   fimEnquanto
   
      escreval("A soma total desses núneros : ", somaTotal, "!")

Fimalgoritmo